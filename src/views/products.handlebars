<div>
    <center>
        <h1>Products</h1>
        
        {{#each docs}}
            <h2>Producto:{{this.title}}</h2>
            <p>Descripcion:{{this.description}}</p>
            <p>imagen:{{this.thumbnail}}</p>
            <p>Precio:{{this.price}}</p>
            <p>Categoria:{{this.category}}</p>
        {{/each}}
        {{#if hasPrevPage}}
            <a href="products?pageQuery={{prevPage}}">Anterior</a>
        {{/if}}
        <label>{{page}}</label>
        {{#if hasNextPage}}
            <a href="products?pageQuery={{nextPage}}">Siguiente</a>
        {{/if}}
        <br>
        <button><a href="/sessions/logout">Logout</button>

    </center>
</div>